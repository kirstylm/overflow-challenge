<h1>Questions</h1>

<%= link_to "Create new question", new_question_path %>

<ul id="questions">
  <% @questions.each do |question| %>
    <li>
      <strong><%= question.title %></strong>
      <p>User who created question: <%= question.user_id %></p>
      <%= link_to "View question", question_path(question) %>
    </li>
  <% end %>
</ul>


<%= will_paginate @questions %>
